    <th>Bal9</th>
    <th>Otvet10</th>
    <th>Bal10</th>
    <th>User otvet</th>
  </tr>
  </thead>

  <tbody>
  <% @ans.each do |an| %>
      <tr>
        <td><%= link_to 'Show', an, class: 'btn btn-info' %></td>
        <td><%= link_to 'Edit', edit_an_path(an), class: 'btn btn-warning' %></td>
        <td><%= link_to 'Destroy', an, method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn btn-danger' %></td>
        <td><%= an.id_test %></td>
        <td><%= an.naim %></td>
        <td><%= an.otvet1 %></td>
        <td><%= an.bal1 %></td>
        <td><%= an.otvet2 %></td>
