        <td><%= an.bal4 %></td>
        <td><%= an.otvet5 %></td>
        <td><%= an.bal5 %></td>
        <td><%= an.otvet6 %></td>
        <td><%= an.bal6 %></td>
        <td><%= an.otvet7 %></td>
        <td><%= an.bal7 %></td>
        <td><%= an.otvet8 %></td>
        <td><%= an.bal8 %></td>
        <td><%= an.otvet9 %></td>
        <td><%= an.bal9 %></td>
        <td><%= an.otvet10 %></td>
        <td><%= an.bal10 %></td>
        <td><%= an.user_otvet %></td>
      </tr>
  <% end %>
