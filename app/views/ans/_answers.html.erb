<%= paginate @ans %>
<table class="table table-bordered table-striped">
  <thead>
  <tr>
    <th colspan="3"></th>
    <th>Id test</th>
    <th>Naim</th>
    <th>Otvet1</th>
    <th>Bal1</th>
    <th>Otvet2</th>
    <th>Bal2</th>
    <th>Otvet3</th>
    <th>Bal3</th>
    <th>Otvet4</th>
    <th>Bal4</th>
    <th>Otvet5</th>
    <th>Bal5</th>
    <th>Otvet6</th>
    <th>Bal6</th>
    <th>Otvet7</th>
